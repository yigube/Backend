{
  "name": "asistencia-backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node --env-file=.env src/server.js",
    "start": "node src/server.js",
    "db:seed": "node --env-file=.env src/seeders/seed.js",
    "db:migrate": "sequelize-cli --config sequelize.config.cjs db:migrate",
    "db:migrate:undo": "sequelize-cli --config sequelize.config.cjs db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli --config sequelize.config.cjs db:migrate:undo:all",
    "test": "set NODE_ENV=test&& set DB_NAME_TEST=asistencia_db_test&& node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "csv-stringify": "^6.5.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "mysql2": "^3.11.3",
    "sequelize": "^6.37.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "sequelize-cli": "^6.6.3",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0"
  }
}
